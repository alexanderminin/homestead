{
    "ip": "192.168.10.10",
    "memory": 2048,
    "cpus": 1,
    "provider": "virtualbox",
    "authorize": "~/.ssh/id_rsa.pub",
    "keys": [
        "~/.ssh/id_rsa"
    ],
    "folders": [
        {
            "map": "/var/www/emsender",
            "to": "/home/vagrant/code/emsender"
        },
        {
            "map": "/var/www/UCSender",
            "to": "/home/vagrant/code/ucsender"
        },
        {
            "map": "/var/www/testapp",
            "to": "/home/vagrant/code/testapp"
        }
    ],
    "sites": [
        {
            "map": "homestead.test",
            "to": "/home/vagrant/code/emsender/public"
        },
        {
            "map": "emsender.test",
            "to": "/home/vagrant/code/emsender/public"
        },
        {
            "map": "ucsender.test",
            "to": "/home/vagrant/code/ucsender/public"
        },
        {
            "map": "testapp.test",
            "to": "/home/vagrant/code/testapp/public"
        }
    ],
    "databases": [
        "homestead"
    ]
}
